      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "chai": "^5.1.2",
